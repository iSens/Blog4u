<h1>Listing posts</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Name</th>
    <th>Post date</th>
    <th>Body</th>

  </tr>

<% @posts.each do |post| %>
  <tr>
	<td><%=link_to post.title, post %>
    <td><%= post.name %></td>
    <td><%= post.post_date %></td>
    <td><%= post.body %></td>
  </tr>
<% end %>
</table>

